<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.springframework.context.support.MyInstantiationAwareBeanPostProcessor"/>

	<bean class="org.springframework.context.support.BeanFactoryProcessorTest">
		<property name="obscenties">
			<set>
				<value>bollocks</value>
				<value>winky</value>
				<value>bum</value>
			</set>
		</property>
	</bean>

	<bean id="bfpp" class="org.springframework.context.support.BeanFactoryProcessorTests.SimplePostProcessor">
		<property name="username" value="bollocks"/>
		<property name="password" value="winky"/>
	</bean>

</beans>
