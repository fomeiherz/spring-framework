<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="showBean"  class="org.springframework.beans.factory.xml.XmlBeanFactoryTest.ShowBean">
		<lookup-method name="getBean" bean="user"/>
	</bean>
	<bean id="teacher" class="org.springframework.beans.factory.xml.XmlBeanFactoryTest.Teacher"/>
	<bean id="user" class="org.springframework.beans.factory.xml.XmlBeanFactoryTest.User"/>
</beans>