<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="classA" class="org.springframework.beans.factory.CircleDependencySetterTests.ClassA">
		<property name="classB" ref="classB"/>
	</bean>
	<bean id="classB" class="org.springframework.beans.factory.CircleDependencySetterTests.ClassB">
		<property name="classC" ref="classC"/>
	</bean>
	<bean id="classC" class="org.springframework.beans.factory.CircleDependencySetterTests.ClassC">
		<property name="classA" ref="classA"/>
	</bean>

</beans>
